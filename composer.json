{
    "name": "neurony/crudhub-lang",
    "description": "Laravel Admin Panel",
    "license": "mit",
    "homepage": "https://crudhub.com",
    "keywords": [
        "laravel",
        "admin panel",
        "multi language",
        "translatable",
        "model translations",
        "static translations",
        "languages",
        "locales"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Andrei Badea",
            "email": "zbiller@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-translatable": "^6.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Zbiller\\CrudhubLang\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zbiller\\CrudhubLang\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zbiller\\CrudhubLang\\CrudhubLangServiceProvider"
            ]
        }
    }
}
